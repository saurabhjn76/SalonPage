<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

<item>
    <shape
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

        <solid
            android:color="#DD111111"/>
        <stroke
            android:width="4dp"
            android:color="#000000" />
        <padding
            android:bottom="30dp"
            android:left="30dp"
            android:right="30dp"
            android:top="30dp"/>

        <size
            android:width="90dp"
            android:height="90dp"/>
    </shape>
</item>

</layer-list>